<com.example.daxinli.tempmusic.view.floatbackground.FloatBackLayout
    android:id="@+id/welcome_floatbackground"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.example.daxinli.tempmusic.musicTouch.MutiGameActivity">

        <LinearLayout
            android:background="@drawable/blackgborder_cursorr"
            android:layout_marginTop="100dp"
            android:layout_width="320dp"
            android:layout_height="250dp"
            android:layout_gravity="center"
            android:orientation="vertical">


            <com.dd.processbutton.FlatButton
                android:id="@+id/btn_createAHome"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/white"
                android:layout_gravity="center"
                android:layout_marginTop="40dp"
                android:text="创建房间"
                android:textSize="30dp" />
            <com.dd.processbutton.FlatButton
                android:id="@+id/btn_enterAHome"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="35dp"
                android:layout_gravity="center"
                android:text="进入房间"
                android:textSize="30dp"
                android:textColor="@android:color/white" />

        </LinearLayout>


    </LinearLayout>
</com.example.daxinli.tempmusic.view.floatbackground.FloatBackLayout>

